#include "TArtSortEvent.h"

void TArtSortEvent::Clear(Option_t *t)
{

  for(int i=0;i<310;i++){
    
    F1FIBER_time[i]  = 0;
    F1FIBER_Y_POS[i] = -50000;
    
  }
  
  F1_Tref = 0;
  
  F2UPPAC_L = 0;
  F2UPPAC_D = 0;
  F2UPPAC_R = 0;
  F2UPPAC_U = 0;
  F2UPPAC_A = 0;
  
  F2DPPAC_L = 0;
  F2DPPAC_D = 0;
  F2DPPAC_R = 0;
  F2DPPAC_U = 0;
  F2DPPAC_A = 0;
  
  F3UPPAC_L = 0;
  F3UPPAC_D = 0;
  F3UPPAC_R = 0;
  F3UPPAC_U = 0;
  F3UPPAC_A = 0;
  // 28052014 TETSUTA ADD
  F3DPPAC_L =	0;
  F3DPPAC_D =	0;
  F3DPPAC_R =	0;
  F3DPPAC_U =	0;
  F3DPPAC_A =	0;
  // ------------------------------
  
  F2_Silicon = 0;
  F3_Silicon = 0;	// 28052014 TETSUTA ADD
  
  F2_Scint_Q = 0;
  F2_Scint_T = 0;

  F2_RF   = 0;
  F2_Tref = 0;
  
  PPAC_Trg = 0;
  
  // 30052014 TETSUTA ADD
  F2PLASTIC	= 0;
  F3PLASTIC_L	= 0;
  F3PLASTIC_R	= 0;
  RFreq         = 0;
  F2PLASTIC_T	= 0;
  F3PLASTIC_L_T	= 0;
  F3PLASTIC_R_T	= 0;
  F2SSD         = 0;
  F3SSD         = 0;
  // ------------------------------	    

  TPC_Silicon_RU  = 0;
  TPC_Silicon_RD  = 0;
  TPC_Silicon_CRU = 0;
  TPC_Silicon_CRD = 0;
  TPC_Silicon_LU  = 0;
  TPC_Silicon_LD  = 0;
  TPC_Silicon_CLU = 0;
  TPC_Silicon_CLD = 0;
  
  for(int i=0;i<24;i++){
    
    TPC_CesiumI_Center[i] = 0;
    
  }

  for(int i=0;i<8;i++){
    
    TPCSi[i] =	0;
    
  }
  
