  GR_L	    = 0;
  GR_R	    = 0;
  RFreq	    = 0;
  
  // TETSUYA 20140522 CAGRAF 
  PLASTIC1    = 0;
  PLASTIC2    = 0;
  GR_TRIG     = 0;
  TOF_RF_Liq1 = 0;
  TOF_RF_Liq2 = 0;
  GE	     = 0;
}
